<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ivanorezac.dumbPunch" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Dumb Punch</name>
